{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "affected": {
    "defaultBase": "master"
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "codegen",
        "^codegen",
        "build",
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "codegen": {
      "cache": true
    },
    "swagger": {
      "cache": true
    },
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore",
        "{workspaceRoot}/eslint.config.js"
      ]
    }
  },
  "plugins": [
    {
      "plugin": "@nx-dotnet/core",
      "options": {
        "solutionFile": "{npmScope}.nx-dotnet.sln",
        "inferProjects": true,
        "inferProjectTargets": true,
        "nugetPackages": {
          "Microsoft.EntityFrameworkCore": "6.0.12",
          "Microsoft.EntityFrameworkCore.Design": "6.0.12",
          "Microsoft.EntityFrameworkCore.Relational": "6.0.12",
          "Microsoft.EntityFrameworkCore.Tools": "6.0.12",
          "Microsoft.Extensions.Configuration.Json": "7.0.0",
          "Newtonsoft.Json": "13.0.2",
          "Pomelo.EntityFrameworkCore.MySql": "6.0.2"
        }
      }
    },
    "@nx-dotnet/core"
  ],
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "sharedGlobals": [],
    "production": [
      "default",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/eslint.config.js"
    ]
  },
  "generators": {
    "@nx/react": {
      "application": {
        "babel": true
      }
    }
  }
}
